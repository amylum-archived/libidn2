    gitlab_tag
    repo 'libidn/libidn2'
    regex /^libidn2-([\d.]+)$/
  end

  actual do
    git_tag
    regex /^(.*)-\d+$/
  end

  extend ProspectusPkgforge::DepItems
  extend ProspectusTravis::Build.new(my_slug)
end
